{
  "name": "library-classification",
  "version": "1.0.0",
  "description": "",
  "main": "./src/app.js",
  "scripts": {
    "clean": "rimraf ./node_modules && rm -rf ./dist",
    "server": "babel-node --presets @babel/preset-env ./src/app.js",
    "build": "rm -rf ./dist && babel ./src -d dist --presets @babel/preset-env",
    "start": "nodemon dist/app.js",
    "dev": "nodemon --exec babel-node --presets @babel/preset-env ./src/app.js",
    "build-deploy": "babel ./src --out-dir ./dist --presets @babel/preset-env",
    "deploy:seed:all": "cd ./dist && npx sequelize-cli db:seed:all --env db --config config/index.js",
    "deploy:seed:undo": "cd ./dist && npx sequelize-cli db:seed:undo:all --env db --config config/index.js",
    "test": "mocha --require @babel/register || true",
    "lint": "eslint ./src",
    "lint:fix": "eslint  --fix ./src",
    "jest": "jest",
    "db:generate": "cd ./src && npx sequelize-cli migration:generate --env db --name",
    "db:migrate": "cd ./src && npx sequelize-cli db:migrate --env db",
    "db:migrate:undo": "cd ./src && npx sequelize-cli db:migrate:undo --env db",
    "seed:all": "cd ./src && npx sequelize-cli db:seed:all --env db",
    "seed:undo": "cd ./src && npx sequelize-cli db:seed:undo:all --env db",
    "seed:generate": "cd ./src && sequelize-cli seed:generate --env db --name"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/node": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.15.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.2.2",
    "chai": "^4.3.4",
    "eslint": "^8.36.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^27.2.1",
    "mocha": "^9.1.2",
    "nodemon": "^2.0.13",
    "sequelize-cli": "^6.6.2",
    "sinon": "^11.1.2",
    "supertest": "^6.1.6",
    "uuid": "^9.0.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "https": "^1.0.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^9.0.0",
    "nanoid": "^3.1.25",
    "nodemailer": "^6.9.1",
    "pg": "^8.7.1",
    "sequelize": "^6.6.5",
    "serialize-javascript": "^6.0.1",
    "shortid": "^2.2.16",
    "swagger-ui-express": "^4.1.6"
  }
}
